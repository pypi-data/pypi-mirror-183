# # Used in MONGO_CONNECTION_STRING ##
MONGODB_USERNAME=mongoroot
MONGODB_PASSWORD=mongopass
MONGODB_CLUSTER_HOSTS=mongodb1:27017,mongodb2:27017,mongodb3:27017

# # configure ##
JASMIN_CLI_HOST=jasmin
JASMIN_CLI_PORT=8990
JASMIN_CLI_TIMEOUT=30
JASMIN_CLI_AUTH=yes
JASMIN_CLI_USERNAME=jcliadmin
JASMIN_CLI_PASSWORD=jclipwd
JASMIN_CLI_STANDARD_PROMPT="jcli : "
JASMIN_CLI_INTERACTIVE_PROMPT="> "

MONGO_CONFIGURATION_DATABASE=
SYNC_CURRENT_FIRST=yes
JASMIN_MONGO_CONFIGURATION_LOG_PATH=/var/log/jasmin/
JASMIN_MONGO_CONFIGURATION_LOG_LEVEL=INFO
